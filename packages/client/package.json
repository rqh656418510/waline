{
  "name": "@waline/client",
  "description": "client for waline comment system",
  "version": "1.0.0-beta.2",
  "keywords": [
    "valine",
    "waline",
    "comment",
    "blog"
  ],
  "browser": "dist/Waline.min.js",
  "main": "dist/Waline.min.js",
  "types": "dist/Waline.min.d.ts",
  "repository": {
    "url": "https://github.com/lizheming/waline"
  },
  "scripts": {
    "analyze": "cross-env ANALYZE=true webpack --mode=production",
    "build": "npm run build:webpack && npm run build:rollup",
    "build:rollup": "rollup -c rollup.config.js",
    "build:webpack": "webpack --mode=production",
    "dev": "webpack serve --mode=development",
    "prepublish": "npm run build"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not ie 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/autosize": "^3.0.7",
    "@types/marked": "^2.0.3",
    "autosize": "^4.0.4",
    "hanabi": "^0.4.0",
    "marked": "^2.0.5",
    "mitt": "^2.1.0",
    "vue": "^3.0.0"
  }
}
